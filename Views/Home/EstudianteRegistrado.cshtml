@model Estudiante
<!DOCTYPE html>
<html lang="es">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta http-equiv="X-UA-Compatible" content="ie=edge">
    <title>Estudiante Registrado- Convivencia</title>
    <link rel="stylesheet" type="text/css" href="css/Estilos.css">
</head>
<body>
    <h1 style="color: lightblue; margin-top: 40px;">Registrar Estudiante > Registro</h1><br/>
     <label><b>DNI o Codigo de estudiante</b></label>
    <div class="form-group">@Html.DisplayTextFor(model=>model.ID)</div><br/>
    <label><b>Apelido Paterno:</b></label>
    <div class="form-group">@Html.DisplayTextFor(model=>model.Apep)</div><br/>
     <label><b>Apellido Materno:</b></label>
    <div class="form-group">@Html.DisplayTextFor(model=>model.Apem)</div><br/>
     <label><b>Nombres</b></label>
    <div class="form-group">@Html.DisplayTextFor(model=>model.Name)</div><br/>
    <label><b>Fecha de nacimiento</b></label>
    <div class="form-group">@Html.DisplayTextFor(model=>model.Birth)</div><br/>
    <label><b>Carrera</b></label>
    <div class="form-group">@Html.DisplayTextFor(model=>model.Carr)</div><br/>
    <label><b>Ciclo</b></label>
    <div class="form-group">@Html.DisplayTextFor(model=>model.Cic)</div><br/><br/>
    <input type="submit" class="btn btn-info" value="Volver" style="margin: auto; display: block; margin-bottom: 40px;" onclick="window.history.back();"/>
</body>
</html>